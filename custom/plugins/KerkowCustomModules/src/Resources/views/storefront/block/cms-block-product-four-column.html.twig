{% block block_product_four_column %}
	{% set columns = 4 %}
	{% set id = element.id %}
	{% block block_product_four_column_one %}
		{% set element = block.slots.getSlot('one') %}

		<div class="col-6 col-md-3 card-col" data-cms-element-id="{{ id }}">
			{% block block_product_four_column_one_inner %}
				{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
			{% endblock %}
		</div>
	{% endblock %}

