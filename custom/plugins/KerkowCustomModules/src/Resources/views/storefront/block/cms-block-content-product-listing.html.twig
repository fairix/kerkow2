{% block block_content_product_listing %}
	{% set element = block.slots.getSlot('products') %}
	{% set content = block.slots.getSlot('content') %}
	{% set animal = block.slots.getSlot('animal') %}
	{% set columns = 1 %}

	<div class="col-12" data-cms-element-id="{{ element.id }}">
		{% block block_content_product_listing_inner %}
			{% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
		{% endblock %}
	</div>
{% endblock %}

